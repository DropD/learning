{
    "extends": ["airbnb-base", "plugin:react/recommended"],
    "env": {
        "browser": "true",
        "node": "true"
    },
    "rules": {
      "no-console": "off",
      "import/extensions": [ "error", "always", { "ignorePackages": true } ],
      "react/prop-types": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
}

